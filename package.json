{
  "name": "germs",
  "version": "0.1.8",
  "description": "an opinionated build toolset that is dead-simple so you can focus on writing code",
  "main": "germs.js",
  "scripts": {
    "dependencies": "nps dependencies",
    "rebuild": "babel src -d lib --ignore *.spec.js",
    "rebundle": "rollup -c rollup/config.commonjs.js",
    "readme": "nps readme",
    "lint": "nps lint",
    "test": "nps test",
    "docs": "nps docs",
    "bundle": "nps bundle",
    "build": "npm run rebuild && npm run rebundle",
    "precommit": "nps care",
    "prepare": "npm run build",
    "prepublish": "npm run build"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/brekk/germs.git"
  },
  "keywords": [
    "build",
    "code",
    "simply"
  ],
  "author": "brekk",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/brekk/germs/issues"
  },
  "homepage": "https://github.com/brekk/germs#readme",
  "jest": {
    "modulePaths": [
      "src"
    ],
    "moduleDirectories": [
      "node_modules",
      "src"
    ],
    "coveragePathIgnorePatterns": [
      "test-helpers.js",
      "index.js"
    ],
    "mapCoverage": true,
    "moduleNameMapper": {
      "@app": "<rootDir>/src/index.js",
      "@testing/(.*)": "<rootDir>/src/testing/$1.js",
      "@tools/(.*)": "<rootDir>/src/tools/$1.js"
    },
    "moduleFileExtensions": [
      "js",
      "json"
    ],
    "testMatch": [
      "**/*.spec.(jsx|js)"
    ]
  },
  "dependencies": {
    "babel-cli": "^6.26.0",
    "babel-core": "^6.26.0",
    "babel-eslint": "8.0.1",
    "babel-jest": "21.0.2",
    "babel-plugin-module-resolver": "^2.7.1",
    "babel-plugin-syntax-async-functions": "^6.13.0",
    "babel-plugin-transform-async-to-generator": "^6.24.1",
    "babel-plugin-transform-es2015-destructuring": "^6.23.0",
    "babel-plugin-transform-object-rest-spread": "^6.26.0",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-fbjs": "^2.1.4",
    "babel-preset-react": "^6.24.1",
    "babel-preset-stage-2": "^6.24.1",
    "babel-register": "^6.26.0",
    "chalk": "^2.1.0",
    "depcheck": "^0.6.7",
    "documentation": "^5.3.2",
    "eslint": "4.6.1",
    "eslint-config-standard": "^10.2.1",
    "eslint-plugin-ava": "^4.2.2",
    "eslint-plugin-babel": "^4.1.2",
    "eslint-plugin-eslint-comments": "^1.0.3",
    "eslint-plugin-fp": "^2.3.0",
    "eslint-plugin-import": "^2.7.0",
    "eslint-plugin-node": "5.1.1",
    "eslint-plugin-promise": "^3.5.0",
    "eslint-plugin-react": "7.3.0",
    "eslint-plugin-standard": "^3.0.1",
    "execa": "^0.8.0",
    "husky": "^0.14.3",
    "jayin": "^0.0.3",
    "jest": "21.0.2",
    "jest-cli": "^21.2.1",
    "nps": "^5.7.1",
    "nps-utils": "^1.3.0",
    "rollup": "0.49.3",
    "rollup-plugin-alias": "^1.3.1",
    "rollup-plugin-babel-minify": "^3.1.2",
    "rollup-plugin-buble": "0.15.0",
    "rollup-plugin-cleanup": "^1.0.1",
    "rollup-plugin-commonjs": "^8.2.1",
    "rollup-plugin-git-version": "^0.2.1",
    "rollup-plugin-node-resolve": "^3.0.0",
    "rollup-plugin-progress": "^0.4.0"
  },
  "devDependencies": {
    "babel-cli": "^6.26.0",
    "babel-core": "^6.26.0",
    "babel-eslint": "8.0.1",
    "babel-jest": "21.0.2",
    "babel-plugin-module-resolver": "^2.7.1",
    "babel-plugin-syntax-async-functions": "^6.13.0",
    "babel-plugin-transform-async-to-generator": "^6.24.1",
    "babel-plugin-transform-es2015-destructuring": "^6.23.0",
    "babel-plugin-transform-object-rest-spread": "^6.26.0",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-fbjs": "^2.1.4",
    "babel-preset-react": "^6.24.1",
    "babel-preset-stage-2": "^6.24.1",
    "babel-register": "^6.26.0",
    "chalk": "^2.1.0",
    "depcheck": "^0.6.7",
    "documentation": "^5.3.2",
    "eslint": "4.6.1",
    "eslint-config-standard": "^10.2.1",
    "eslint-plugin-ava": "^4.2.2",
    "eslint-plugin-babel": "^4.1.2",
    "eslint-plugin-eslint-comments": "^1.0.3",
    "eslint-plugin-fp": "^2.3.0",
    "eslint-plugin-import": "^2.7.0",
    "eslint-plugin-node": "5.1.1",
    "eslint-plugin-promise": "^3.5.0",
    "eslint-plugin-react": "7.3.0",
    "eslint-plugin-standard": "^3.0.1",
    "execa": "^0.8.0",
    "husky": "^0.14.3",
    "jayin": "^0.0.3",
    "jest": "21.0.2",
    "jest-cli": "^21.2.1",
    "nps": "^5.7.1",
    "nps-utils": "^1.3.0",
    "rollup": "0.49.3",
    "rollup-plugin-alias": "^1.3.1",
    "rollup-plugin-babel-minify": "^3.1.2",
    "rollup-plugin-buble": "0.15.0",
    "rollup-plugin-cleanup": "^1.0.1",
    "rollup-plugin-commonjs": "^8.2.1",
    "rollup-plugin-git-version": "^0.2.1",
    "rollup-plugin-node-resolve": "^3.0.0",
    "rollup-plugin-progress": "^0.4.0"
  }
}
